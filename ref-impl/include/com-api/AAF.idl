  // 
  HRESULT GetRepresentation (
    // Slot ID
    [in] aafSlotID_t  slotID,

    // Index of requested representation
    [in] aafUInt32  index,

    // Requested Source Clip
    [out] IAAFSegment ** ppSourceClip);


