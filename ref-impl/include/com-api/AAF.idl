  HRESULT GetLastModified (
    [out, retval] aafTimeStamp_t *  pTimeStamp);


  //***********************************************************
  //
  // GetContentStorage()
  //
  /// Places the Content Storage object attached to the header into the
  /// *ppStorage argument.
